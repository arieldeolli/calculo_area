<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ariel.calculoarea" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/calculadora"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResultadoFinalActivity" />
        <activity android:name=".RetanguloActivity" />
        <activity android:name=".TrianguloActivity" />
        <activity android:name=".CirculoActivity"></activity>
    </application>


</manifest>
